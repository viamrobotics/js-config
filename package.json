{
  "name": "@viamrobotics/js-config",
  "private": true,
  "packageManager": "pnpm@8.1.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "all": "concurrently -g pnpm:check-* pnpm:build",
    "check-format": "pnpm run _prettier-base --check",
    "check-lint": "eslint \"**/*.@(js|cjs|ts)\"",
    "check-types": "tsc",
    "build": "pnpm run -r --aggregate-output build",
    "format": "pnpm run _prettier-base --write",
    "_prettier-base": "prettier \"**/*.@(js|cjs|ts|json|yaml|md)\""
  },
  "dependencies": {
    "@viamrobotics/eslint-config": "workspace:*",
    "@viamrobotics/prettier-config": "workspace:*",
    "@viamrobotics/typescript-config": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "prettier": "^2.8.7",
    "typescript": "^5.0.2"
  }
}
